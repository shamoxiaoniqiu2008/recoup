   <select id="selectRingRecords" parameterType="java.util.Map" resultMap="ExtandResultMap">
  SELECT record.*,room.id AS room_id,room.name as room_name,e.id as nurse_id, e.name as nurse_name
	from pension_ringrecord record 
	LEFT JOIN pension_ring ring 
	ON record.ring_id = ring.id
	LEFT JOIN pension_room room
	ON ring.room_id = room.id
	LEFT JOIN pension_employee e
	ON ring.nurse_id = e.id
  where 1=1
  <if test="startDate != null">
			and unix_timestamp(record.ringTime)
			&gt;=unix_timestamp(#{startDate}) 
		</if>
		<if test="endDate != null">
			and unix_timestamp(date_add(#{endDate}, interval 1 day))
			&gt;=unix_timestamp(record.ringTime)
		</if>
  <if test=" ringstatus != null">
   and record.ringStatus = #{ringstatus,jdbcType=INTEGER}
  </if>
  and record.cleared = 2
  order by record.ringTime
  
  </select>
  
