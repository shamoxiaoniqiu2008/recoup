	<!-- 查询医生巡诊记录 add by wensy.yang -->
	<select id="selectDiagnoseRecords" parameterType="map"
		resultType="service.olderManage.PensionDiagnoseDomain">
		select
		pd.*,pd.recorder_id as recorderId,
		pd.older_id as olderId,po.`name` as olderName,
		pe.`name` as recorderName,
		pd.pulse_rate as pulseRate
		from
		pension_diagnosis pd,
		pension_older po,
		pension_employee pe
		where
		pd.older_id=po.id
		and pe.id=pd.recorder_id
		and
		pd.cleared=2
		<if test="olderId!=null">
			and po.id=#{olderId,jdbcType=INTEGER}
		</if>
		<if test="startDate != null &amp;&amp; startDate != ''">
			and pd.diagnosisTime &gt;= #{startDate,jdbcType=TIMESTAMP}
		</if>
		<if test="endDate != null &amp;&amp; endDate != ''">
			and unix_timestamp(date_add(#{endDate}, interval 1 day))
			&gt;=unix_timestamp(pd.diagnosisTime)
		</if>
	</select>

