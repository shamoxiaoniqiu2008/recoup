	<select id="selectCareAppList" parameterType="map"
		resultType="service.olderManage.CareAppointmentDomain">
		select
		pca.id,
		pca.older_id as olderId,
		pca.older_name as olderName,
		pca.appointment_time as appointmentTime,
		pe.name as respNurseName,
		pd.nurser_name as executeNurseName,
		pd.id as daycareId,
		pb.name as bedName,
		pb.roomName,
		pb.floorName,
		pb.buildName,
		pca.family_sign as familySign,
		pca.generate_flag as
		generateFlag,
		pca.send_flag as sendFlag,
		if(pca.family_sign=1,true,false) as signFlag,
		if(pca.generate_flag=1,true,false) as generFlag,
		if(pca.send_flag=1,true,false) as sendF

		from
		pension_care_appointment
		pca
		left join
		pension_daycare pd
		on
		pca.id=pd.nurse_id
		and pd.nurse_type=2,
		pension_livingrecord pl,
		pension_bed pb,
		pension_employee pe

		where
		pca.older_id=pl.older_id
		and pl.bed_id=pb.id
		and pe.id=pl.nurse_id
		and
		pca.cleared=2

		<if test="startDate != null">
			and unix_timestamp(pca.appointment_time)
			&gt;=unix_timestamp(#{startDate}) 
		</if>
		<if test="endDate != null">
			and unix_timestamp(date_add(#{endDate}, interval 1 day))
			&gt;=unix_timestamp(pca.appointment_time)
		</if>
		<if test="olderId!=null">
			and pca.older_id=#{olderId,jdbcType=INTEGER}
		</if>
		<if test="careAppId!=null">
			and pca.id=#{careAppId,jdbcType=INTEGER}
		</if>
		order by pca.appointment_time desc
	</select>
