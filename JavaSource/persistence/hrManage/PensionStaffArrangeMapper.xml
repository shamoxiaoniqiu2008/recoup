  <select id="selectStaffArranges" parameterType="java.util.Map" resultMap="BaseResultMap">
		SELECT *

		from 
		
		pension_staff_arrange 

		where

		1=1
		
		<if test="startDate != null"> 
		
		and unix_timestamp(arrange_time)
			&gt;=unix_timestamp(#{startDate})
	</if>
	<if test="endDate != null"> 
		
		and unix_timestamp(date_add(#{endDate}, interval 1 day))
			&gt;unix_timestamp(arrange_time)
	</if>
	<if test="deptId != null"> 
		
		and depart_id=#{deptId,jdbcType=DECIMAL}
	</if>
	<if test="empId != null"> 
		
		and employee_id=#{empId,jdbcType=DECIMAL}
	</if>

		and cleared = 2 ORDER BY ARRANGE_TIME
		
	</select>
