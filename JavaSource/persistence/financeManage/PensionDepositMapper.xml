  <!-- 查询押金缴纳主记录   add by Mary 2013-9-23 -->

	<select id="selectDepositList" parameterType="java.util.Map" resultMap="BaseResultMap">

select deposit.id as id,
deposit.older_id as older_id,
older.`name` as older_name,
bed.buildName as building_name,
bed.floorName as floor_name,
bed.roomName as room_name,
bed.`name` as bed_name,
deposit.deposit as deposit,
older.statuses as status,
case older.statuses WHEN 3 then '入住' when 4 then '请假' WHEN 5 then '待结算' WHEN 6 then  '已离院' end as status_name
from pension_deposit deposit 
LEFT JOIN pension_older older ON older.id = deposit.older_id
LEFT JOIN pension_livingrecord record ON record.older_id = older.id
LEFT JOIN pension_bed bed ON bed.id = record.bed_id
where 1 = 1 		
		<if test="depositId != null"> 
		
		and deposit.id=#{depositId,jdbcType=DECIMAL}
	</if>
	<if test="status != null"> 
		
		and older.statuses=#{status,jdbcType=DECIMAL}
	</if>
	<if test="depositStatus == 1"> 
		and deposit = 0
	</if>
	<if test="depositStatus == 2"> 
		and deposit &gt; 0
	</if>
	
	order by deposit.id DESC

	</select>
	
	
