  <!-- 获取当前人员可以审批的财务申请add by Mary 2013-9-10   -->

  <select id="selectApprovalBudgets" parameterType="java.util.Map" resultMap="ExtandResultMap">
  
 SELECT f.*,
budgetType.type as budgetTypeName,
CASE f.approveResult WHEN 1 THEN '同意' WHEN 2 THEN '不同意' ELSE '正在审批' end as approveResultStr,
IF(f.giver_id,'是','否')  as isSend
from pension_financialbudget f
LEFT JOIN pension_dict_budgettype budgetType ON budgetType.id = f.budgetType
LEFT JOIN pension_approvalbudget a ON f.id = a.budget_id
where 1 = 1
 <if test="startDate != null">
			and unix_timestamp(f.applyTime)
			&gt;=unix_timestamp(#{startDate}) 
		</if>
		<if test="endDate != null">
			and unix_timestamp(date_add(#{endDate}, interval 1 day))
			&gt;=unix_timestamp(f.applyTime)
		</if>
<if test="approvalResult!=null">
	and f.approveResult = #{approvalResult,jdbcType=INTEGER}
</if>
<if test=" sysUserId != null">
	and ( a.approval_id = #{sysUserId,jdbcType=INTEGER}
	or a.dept_id = #{deptId,jdbcType=INTEGER} )
	</if>
	
	and f.cleared = 2
GROUP BY f.id

ORDER BY f.ID DESC
  </select>
