  <!-- 按照起止时间，员工编号，员工部门查询 押金缴纳详情 add by mary.liu 2013-11-10 -->
  
 <select id="selectBalanceDepositLogs" parameterType="java.util.Map" resultMap="BaseResultMapBalanceExtend">
	select pdg.*,pd.older_name as older_name,dept.name as dept_name 
from pension_deposit_log pdg
left join pension_deposit pd
on pdg.deposit_id = pd.id
left join pension_employee emp
on emp.id = pdg.trader_id
left join pension_dept dept
on dept.id = emp.dept_id
where 1=1
<if test="settleFlag != null">
	and pdg.settled_flag = #{settleFlag,jdbcType=INTEGER}
	</if>
	<if test="empId != null">
 	and emp.id =  #{empId,jdbcType=DECIMAL}
	</if>
	<if test="deptId != null">
	and dept.id =  #{deptId,jdbcType=DECIMAL}
	</if>
	<if test="balanceStartDate != null">
			and unix_timestamp(pdg.trade_date)
			&gt;=unix_timestamp(#{balanceStartDate})
		</if>
		<if test="balanceEndDate != null">
			and unix_timestamp(pdg.trade_date)
			&lt;=unix_timestamp(#{balanceEndDate})
		</if>
		

order by pdg.trade_date 

	
	</select>
