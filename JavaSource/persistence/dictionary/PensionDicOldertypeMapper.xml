  <!-- 查询起至时间段内 每种类型老人的数量 add by mary 2013-12-12 -->
  <select id="selectDicOlderTypes" resultMap="BaseResultMap"  parameterType="java.util.Map" >
  select olderType.id as id,
  olderType.oldertype_name as oldertype_name,
  olderType.input_code as input_code,
  olderType.valid_flag as valid_flag,
  olderType.cleared as cleared,
  olderType.note as note,
 count(olderType.id) as num
from pension_older older 
inner join pension_dic_oldertype olderType
on older.types = olderType.id
left join pension_livingrecord pl
on older.id = pl.older_id
left join pension_departregister pd
on pd.older_id = older.id
where older.cleared = 2
and unix_timestamp(pl.visitTime) &lt;= unix_timestamp(#{endDate})
and ( pd.real_leaveTime is null or unix_timestamp(pd.real_leaveTime) &gt; unix_timestamp(#{startDate}))
and ( pd.isAgree is null or pd.isAgree = 6 ) 
group by olderType.id
union
select * from pension_dic_oldertype pdo
where pdo.id not in
(
select olderType.id as id
from pension_older older 
inner join pension_dic_oldertype olderType
on older.types = olderType.id
left join pension_livingrecord pl
on older.id = pl.older_id
left join pension_departregister pd
on pd.older_id = older.id
where older.cleared = 2
and unix_timestamp(pl.visitTime) &lt;= unix_timestamp(#{endDate})
and ( pd.real_leaveTime is null or unix_timestamp(pd.real_leaveTime) &gt; unix_timestamp(#{startDate}))
and ( pd.isAgree is null or pd.isAgree = 6) 
group by olderType.id
)
group by id
  </select>
